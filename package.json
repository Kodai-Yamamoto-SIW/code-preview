{
    "name": "@kodai-yamamoto-siw/code-preview",
    "version": "0.1.0",
    "description": "Docusaurus用のシンプルなHTML/CSSライブプレビューコンポーネント",
    "private": false,
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./styles.css": "./dist/styles.css"
    },
    "files": [
        "dist"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "scripts": {
        "build": "rollup -c",
        "clean": "rimraf dist",
        "prepare": "npm run build"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18"
    },
    "dependencies": {
        "@monaco-editor/react": "^4.7.0"
    },
    "devDependencies": {
        "typescript": "~5.6.2",
        "rollup": "^4.21.2",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-typescript": "^12.1.2",
        "rollup-plugin-postcss": "^4.0.2",
        "postcss": "^8.4.47",
        "autoprefixer": "^10.4.20",
        "tslib": "^2.7.0",
        "rimraf": "^6.0.1",
        "@types/react": "^18.2.28",
        "@types/react-dom": "^18.2.8"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Kodai-Yamamoto-SIW/programming-course-docs"
    }
}